
def findSubstring(s, k):

    my_map = {}
    for i in range(len(s) - k + 1):
        temp = my_map.get(s[i : i + k], 0)
        my_map[s[i : i + k]] = temp + 1
    return my_map

k = int(input().strip())
A = input().strip()
B = input().strip()

A_map = findSubstring(A, k)
B_map = findSubstring(B, k)

count = 0
for item in A_map.items():
    if item[0] in B_map.keys():
        count += B_map[item[0]]

print(count)
