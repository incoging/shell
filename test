n = int(input().strip())


schs = list()
for _ in range(0, n, 2):
    tmp1, tmp2 = [float(input().strip()) for _ in range(2)]
    schs.append((tmp1, tmp2))

schs.sort(key=lambda x: x[1])
right = -1
ans = 0


for p in schs:
    if p[0] <= right:
        ans += 1
    else:
        right = p[1]

print(ans)
