string_input = input().strip()

def restore(string, temp, res, count, i, length):
    if i ==length and count == 0:
        res.append(temp)
        return res
    if (count== 0 and i < length) or (count > 0 and i == length):
        return res
    mask = True
    num = 0
    while i < length and mask:
        if num == 0 and string[i] == "0":
            mask = False
        num = num * 10 + ord(string[i]) - ord('0')
        i += 1
        if num < 256:
            record = ""
            if num == 1:
                record = temp + str(num)
            else:
                record = temp + str(num) + "."
            restore(string, record, res, count-1, i, length)
    return res

def compute(string_):
    result = []
    temp = ""
    the_length = len(string_)
    result = restore(string_, temp, result,4, 0,  the_length)
    print(len(result))

compute(string_input)
